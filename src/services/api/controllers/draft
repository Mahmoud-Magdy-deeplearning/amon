  Coin.prototype.filterKeys = function (price=false) {
    const obj = this.toObject();
    const filtered = {}
    if(price){
    filtered = pick(obj, 'id', 'name', 'code', 'price');
    }
    else {
    filtered = pick(obj, 'id', 'name', 'code');
    }
    return filtered;
  };


